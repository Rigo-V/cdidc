#: cdidc.c:77
#, c-format
msgid "%s: Failed to start %s: %s\n"
msgstr "%s: %s:n käynnistäminen epäonnistui: %s\n"

#: cdidc.c:81
#, c-format
msgid "Submission URL: %s\n"
msgstr "Lähetyslinkki: %s\n"

#: cdidc.c:98
#, c-format
msgid "Usage: %s [OPTIONS]\n"
msgstr "Käyttö: %s [VALINNAT]\n"

#: cdidc.c:99
msgid "Calculate MusicBrainz or CDDB IDs of a compact disc."
msgstr "Laske CD-levyn MusicBrainz- tai CDDB-tunniste."

#: cdidc.c:100
msgid "If neither type of ID is specified, both are printed.\n"
msgstr "Jos kumpaakaan tunnistetyyppiä ei pyydetä, molemmat tulostetaan.\n"

#: cdidc.c:102
#, c-format
msgid "-d DEVICE   Optical disc drive to use. Defaults to %s\n"
msgstr "-d LAITE    Käytettävä optinen asema. Oletuksena %s\n"

#: cdidc.c:104
msgid "-c          Print CDDB ID of CD"
msgstr "-c          Tulostaa CDDB-tunnisteen"

#: cdidc.c:105
msgid "-m          Print MusicBrainz Disc ID of CD"
msgstr "-m          Tulostaa MusicBrainz-tunnisteen"

#: cdidc.c:106
msgid "-s          Submit Disc ID to MusicBrainz using the default browser"
msgstr "-s          Lähetä levyn tunniste MusicBrainziin oletusselaimella"

#: cdidc.c:107
msgid "-w BROWSER  Use BROWSER instead of the system default (implies -s)"
msgstr "-w SELAIN   Käytä SELAINta oletusselaimen sijaan (implikoi -s)"

#: cdidc.c:108
msgid "-b          Format the output more briefly"
msgstr "-b          Tulosta tiiviimmin"

#: cdidc.c:109
#, c-format
msgid "-v          Print version (%s) information and exit\n"
msgstr "-v          Tulosta versiotiedot (%s) ja poistu\n"

#: cdidc.c:111
msgid "-h          Print this help message and exit"
msgstr "-h          Tulosta tämä ohje ja poistu"
